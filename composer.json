{
    "name": "libre-informatique/media-bundle",
    "type": "symfony-bundle",
    "description": "Media Bundle for Symfony",
    "require": {
        "php": ">=5.6",
        "symfony/symfony": ">=3.2",
        "symfony/event-dispatcher": ">=3.2",
        "blast-project/core-bundle": "self.version"
    },
    "require-dev": {
        "symfony/phpunit-bridge": ">=3.2"
    },
    "license": "LGPL-3.0",
    "keywords": [
        "e-venement",
        "libio",
        "sonata",
        "symfony",
        "media"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyLibrinfoMediaBundle",
    "authors": [
        {
            "name": "Romain SANCHEZ",
            "email": "romain.sanchez@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev",
            "dev-wip-lisem": "0.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Librinfo\\MediaBundle\\": "src/"
        }
    }
}
